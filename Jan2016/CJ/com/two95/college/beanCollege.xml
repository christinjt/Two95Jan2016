<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans   
http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="student1" class="com.two95.college.Student">
		<constructor-arg value="CJ"></constructor-arg>	
	</bean>
	<bean id="student2" class="com.two95.college.Student">
		<constructor-arg value="Hri"></constructor-arg>
	</bean>
	<bean id="student3" class="com.two95.college.Student">
		<constructor-arg value="Jisha"></constructor-arg>
	</bean>
	<bean id="student4" class="com.two95.college.Student">
		<constructor-arg value="Nish"></constructor-arg>
	</bean>
<bean id="student5" class="com.two95.college.Student">
		<constructor-arg value="CJT"></constructor-arg>	
	</bean>
	<bean id="student6" class="com.two95.college.Student">
		<constructor-arg value="Hridya"></constructor-arg>
	</bean>
	<bean id="student7" class="com.two95.college.Student">
		<constructor-arg value="JishaK"></constructor-arg>
	</bean>
	<bean id="student8" class="com.two95.college.Student">
		<constructor-arg value="Nishala"></constructor-arg>
	</bean>
<bean id="trainer1" class="com.two95.college.Trainer">
		<constructor-arg value="Ansari"></constructor-arg>
	</bean>
	<bean id="trainer2" class="com.two95.college.Trainer">
		<constructor-arg value="Sabarish"></constructor-arg>
	</bean>
	<bean id="trainer3" class="com.two95.college.Trainer">
		<constructor-arg value="Sabu"></constructor-arg>
	</bean>
	<bean id="trainer4" class="com.two95.college.Trainer">
		<constructor-arg value="Janu"></constructor-arg>
	</bean>
	<bean id="course1" class="com.two95.college.Courses">
		<constructor-arg value="Java"></constructor-arg>
		<constructor-arg>
			<list>
				<ref bean="trainer1" />
				<ref bean="trainer2" />
			</list>
		</constructor-arg>
		<constructor-arg>
			<list>
				<ref bean="student1" />
				<ref bean="student2" />
				<ref bean="student3" />
				<ref bean="student4" />
			</list>
		</constructor-arg>
	</bean>
<bean id="course2" class="com.two95.college.Courses">
		<constructor-arg value="DOTNET"></constructor-arg>
		<constructor-arg>
			<list>
				<ref bean="trainer3" />
				<ref bean="trainer4" />
			</list>
		</constructor-arg>
		<constructor-arg>
			<list>
				<ref bean="student5" />
				<ref bean="student6" />
				<ref bean="student7" />
				<ref bean="student8" />
			</list>
		</constructor-arg>
	</bean>
	<bean id="college" class="com.two95.college.College">
		<constructor-arg value="two95Intl"></constructor-arg>
		<constructor-arg>
			<list>
				<ref bean="course1" />
				<ref bean="course2" />
				</list>
		</constructor-arg>
	</bean>
</beans>  